    }
  },
  "keywords": [
    "react",
    "near-ui"
  ],
  "author": {
    "name": "bytrustu",
    "email": "bytrustu@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bytrustu/near-ui.git"
  },
  "scripts": {
