{
  "name": "near-ui",
  "description": "Next-step Near React UI library",
  "version": "0.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "./browser/specific/main.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
